package ch04_ex01;

public class If02 {

	public static void main(String[] args) {
		int a = Integer.parseInt(args[0]);
		// int b = arg[1];
		// 파싱하는 방법 제대로 알기
		int b = Integer.parseInt(args[1]);
		int c = Integer.parseInt(args[2]);
		
		System.out.println("A=>" +a);
		System.out.println("B=>" +b);
		System.out.println("C=>" +c);
	}

}
