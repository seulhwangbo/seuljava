package ch02;

public class FloatDoubleEx {

	public static void main(String[] args) {
		// 실수값 저장
		// 아무 정의 없이 사용된 소수점은 double 인식한다
		double var1 = 3.14;
		// float  var2 = 3.14; -> 그냥 선언시 기본은 double
		float  var2 = 3.14f;
		
		System.out.println("var1: " + var1);
		System.out.println("var2: " + var2);
	}

}
