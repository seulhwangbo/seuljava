package ch02_ex03;

public class ConditionalOperationEx {

	public static void main(String[] args) {
		
		int score = 60;
		char grade = (score> 90)? 'A' : ((score>80)? 'B' : ((score>70)?'C':'D'));
		// (()) -> 하나의 로직 : 80보다 크면 B, 아니면 C
		System.out.println(score + " 정은은 "+ grade + " 등급입니다.");
	}

}
