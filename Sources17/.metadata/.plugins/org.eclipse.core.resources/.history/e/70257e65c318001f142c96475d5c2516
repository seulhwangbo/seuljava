package ch03_ex01;

public class LogicalOperatorEx {

	public static void main(String[] args) {
		int charCode1 = 'A';
		int charCode2 = 'a';
		
		if( (charCode1>=65) & (charCode1<=90)) {
			System.out.println("대문자 이군요");
		}
		if( (charCode2>=97) && (charCode2<=122)) {
			System.out.println("소문자 이군요");
		}

	}

}
