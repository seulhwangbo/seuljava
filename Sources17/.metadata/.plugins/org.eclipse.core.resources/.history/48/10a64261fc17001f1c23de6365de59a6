package ch02;

public class OperationsPromotionEx {

	public static void main(String[] args) {
		byte byteValue1 = 10;	
		byte byteValue2 = 20;	
		
		//byte byteValue3 = byteValue1 + byteValue2 ;
		//컴파일 에러 발생 확인
		
		int intValue1 = byteValue1 + byteValue2;
		System.out.println("intValue1->"+intValue1);
	}

}
